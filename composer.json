{
    "name": "excell-mobility/broadcasting-service",
    "description": "Sends the messages!",
    "type": "project",
    "homepage": "https://www.excell-mobility.de",
    "authors": [
        {
            "name": "Andr√© Rademacher",
            "role": "Lead Developer"
        }
    ],
